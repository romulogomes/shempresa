<!DOCTYPE html>
<html lang="pt">
<head>
		<title>SIG - Sistema de Informação Gerenciais</title>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimum-scale=1.0, maximum-scale=1.0">
		<meta name="apple-mobile-web-app-capable" content="yes">
		<meta name="apple-mobile-web-app-status-bar-style" content="black">
	
		<link rel="stylesheet" href="assets/css/main.css">
		<link rel="stylesheet" href="assets/css/theme_light.css" type="text/css" id="skin_color">
		<link rel="shortcut icon" href="img/favicon.ico" />
		
		<link rel="stylesheet" href="css/style_down_list.css">
		
		<script src="js/angular.min.js"></script>
		<script src="js/req_sesa.js"></script>
		
	</head>
	<body class="navigation-small" id="bodi" ng-app="sig_app" ng-controller="meu_controller">
		<div class="navbar navbar-inverse navbar-fixed-top">
			<div class="container">
				
				<header-left></header-left>

				<div class="navbar-tools">
					<!-- MENU direita-->
					<header-menu usuario="{{usuario()}}"></header-menu>
				</div>
			</div>
		</div>

			<div class="main-navigation navbar-collapse collapse">
				<div class="navigation-toggler navigation-small">
					<i class="clip-chevron-left"></i>
					<i class="clip-chevron-right"></i>
				</div>
				
				<ul class="main-navigation-menu">
					<!--Menu-->
					<li ng-repeat="menu in menus">
						<!-- Se nao tiver submenu -->
						<a ng-href="{{menu.link}}" ng-hide="menu.submenu"><i class="{{menu.icone}}"></i>
							<span class="title"> {{menu.nome}} </span><i class="icon-arrow" ng-show="menu.submenu"></i>
						</a>
						<!-- Se tiver submenu -->
						<a href="" ng-show="menu.submenu"><i class="{{menu.icone}}"></i>
							<span class="title"> {{menu.nome}} </span><i class="icon-arrow" ng-show="menu.submenu"></i>
						</a>
						<ul class="sub-menu">
							<li ng-show="menu.submenu" ng-repeat="sub in menu.submenu">
								<a href="{{sub.link}}">
									<span class="title"> {{sub.titulo }}  </span>
								</a>
							</li>
						</ul>
					</li>
				</ul>
			</div>
			<!-- start: PAGE -->
			<div class="main-content">

				<!-- Titulo Sistema -->
				<div class="container">
					<div class="row">
						<div class="col-sm-12">
							<ol class="breadcrumb"></ol>
							<div class="page-header">
								
								<onde-estou pagina='Evolução Grupo SES' icone="clip-stats"></onde-estou>
								<titulo-sistema></titulo-sistema>
							</div>
						</div>
					</div>

					<!-- Conteudo -->
					<div class="row" ng-init="comparativo = false">
						<div class="col-sm-10" ng-hide="comparativo">
							<div class="panel panel-default">
								<div class="panel-heading">
									<i class="clip-stats"></i>
										Evolução Grupo SES (Quantitativo)
									<div class="panel-tools">
										<a class="btn btn-xs btn-link panel-collapse collapses" href="#">
										</a>
									</div>
								</div>
								<div class="panel-body">
									<div> 
										<button class="btn_comparativo" ng-click="gera_comparativo()" style=" "> Gerar Comparativo </button>
									</div>
									<div id="dd" class="wrapper-dropdown-3" tabindex="1">
										
										<span>Escolha o cargo</span>
										<ul class="dropdown">
											
											<li class="down_list" ng-repeat="cargo in cargos">
											     <a href="#" ng-click="gera_dat(cargo.nome, '1')">{{cargo.nome}}</a>
											</li>
											
										</ul>

									</div>
									<div id="evolucao_ses">
										<div id="grafico1">
							                   <canvas id="canvas1"></canvas>
							                   <div id="loading" ng-show="loading">
												<div id="loading_centraliza">
													<img src="img/ajax_loading.gif">
												</div>
											</div>
							               </div>
									</div>
								</div>
							</div>
						</div>
					</div>	
						
						<div class="row" ng-show="comparativo">
							<!-- Grafico 2 -->
							<div class="col-sm-6">
								<div class="panel panel-default">
									<div class="panel-heading">
										<i class="clip-users-2"></i>
										Evolução Grupo SES (Quantitativo)
									<div class="panel-tools">
										<a class="btn btn-xs btn-link panel-collapse collapses" href="#">
										</a>
										
										
									</div>
								</div>
								<div class="panel-body">
									
									<div id="dd2" class="wrapper-dropdown-3" tabindex="1">
										<span>Escolha o cargo</span>
										<ul class="dropdown">
											
											<li class="down_list" ng-repeat="cargo in cargos">
											     <a href="#" ng-click="gera_dat(cargo.nome, '2')">{{cargo.nome}}</a>
											</li>
										</ul>
									</div>
									<div id="evolucao_ses">
										<div id="grafico2">
							                   <canvas id="canvas2"></canvas>
							                   <div id="loading" ng-show="loading">
												<div id="loading_centraliza">
													<img src="img/ajax_loading.gif">
												</div>
											</div>
							               </div>
									</div>
									
								</div>
							</div>
						  </div>
						
						
						<!-- Grafico 3 -->
						
							<div class="col-sm-6">
								<div class="panel panel-default">
									<div class="panel-heading">
										<i class="clip-users-2"></i>
										Evolução Grupo SES (Quantitativo)
										<div class="panel-tools">
											<a class="btn btn-xs btn-link panel-collapse collapses" href="#">
											</a>
											
											
										</div>
									</div>
									<div class="panel-body">
										
										<div id="dd3" class="wrapper-dropdown-3" tabindex="1">
											<span>Escolha o cargo</span>
											<ul class="dropdown">
												<li class="down_list" ng-repeat="cargo in cargos">
												     <a href="#" ng-click="gera_dat(cargo.nome, '3')">{{cargo.nome}}</a>
												</li>
											</ul>
										</div>
										
											<div id="evolucao_ses">
												<div id="grafico3">
								                    <canvas id="canvas3"></canvas>
								                    <div id="loading" ng-show="loading">
														<div id="loading_centraliza">
															<img src="img/ajax_loading.gif">
														</div>
													</div>
								                </div>
											</div>
										
									</div>
								</div>
						
							</div>
						</div>
					</div>
				</div>

		<footer-sesa></footer-sesa>
		
		<script src="assets/plugins/jQuery-lib/2.0.3/jquery.min.js"></script>
		<script src="assets/plugins/bootstrap/js/bootstrap.min.js"></script>
		<script src="assets/plugins/iCheck/jquery.icheck.min.js"></script>
		<script src="assets/plugins/perfect-scrollbar/src/jquery.mousewheel.js"></script>
		<script src="assets/plugins/perfect-scrollbar/src/perfect-scrollbar.js"></script>
		<script src="assets/plugins/bootstrap-colorpalette/js/bootstrap-colorpalette.js"></script>
		<script src="assets/js/main.js"></script>

		<script src="js/Chart.min.js"></script>
		<script src="js/modernizr.js"></script>
		<script src="js/down_list.js"></script>

		<script src="js/controller.js"></script>
		<script>
			jQuery(document).ready(function() {
				Main.init();
			});

			

		</script>

	</body>
</html>